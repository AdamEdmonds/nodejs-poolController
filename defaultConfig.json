            "pump": {
                "enabled": false,
                "includeActions": [],
                "includeSource": [],
                "includeDest": [],
                "excludeActions": [],
                "excludeSource": [],
                "excludeDest": []
            },
            "chlorinator": {
                "enabled": true,
                "includeSource": [],
                "includeDest": [],
                "excludeSource": [],
                "excludeDest": []
            },
            "unknown": {
                "enabled": true,
                "includeSource": [],
                "includeDest": [],
                "excludeSource": [],
                "excludeDest": []
            }
        },
        "app": {
            "enabled": true,
            "level": "info"
        }
    },
    "appVersion": "0.0.1"
