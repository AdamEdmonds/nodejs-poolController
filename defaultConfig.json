                "httpsRedirect": false,
                "authentication": "none",
                "authFile": "/users.htpasswd"
            },
            "https": {
                "enabled": true,
                "ip": "127.0.0.1",
                "port": 4201,
                "authentication": "none",
                "authFile": "/users.htpasswd",
                "sslKeyFile": "",
                "sslCertFile": ""
            },
            "mdns": {
                "enabled": false
            },
            "ssdp": {
                "enabled": false
            }
        },
        "services": {}
    },
    "log": {
        "packet": {
            "enabled": false,
            "logToConsole": false,
            "logToFile": false,
