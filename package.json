{
  "name": "nodejs-poolcontroller",
  "version": "6.0.0",
  "description": "NodeJS program to read and write to the the pool equipment serial bus.",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=production node src/index.js",
    "dev": "node src/index.js",
    "build": "parcel build src/www/pages/index.html -d src/www/dist",
    "start:capture": "node src/index.js $* --capturePackets",
    "start:replay": "node src/index.js $* --suppressWrite",
    "test": "snyk test && npm run build && NODE_ENV=production mocha ./specs/assets/config/config.json ./specs/index.spec.js --recursive  ./specs  --reporter spec --require ./specs/helpers/chai.js --exit",
    "test:one": "npm run build && NODE_ENV=production mocha  ./specs/assets/config/config.json --reporter spec --require ./specs/helpers/chai.js ./specs/index.spec.js --exit",
    "test:one:cover": "npm run build && NODE_ENV=production istanbul cover  _mocha -- ./specs/assets/config/config.json ./specs/index.spec.js --include-all-sources  ./specs  --require ./specs/helpers/chai.js --reporter spec --exit",
    "test:cover": "npm run build && NODE_ENV=production istanbul cover  _mocha -- ./specs/assets/config/config.json ./specs/index.spec.js --include-all-sources --recursive  ./specs  --require ./specs/helpers/chai.js --reporter spec --exit",
    "test:replay": "mocha  ./specs/assets/config/config.json --reporter spec --require ./specs/helpers/chai.js ./specs/index.spec.js ./misc/replay.js --exit --delay",
    "start:cover": "npm run build && NODE_ENV=production istanbul cover --handle-sigint src/index.js",
    "open:cover": "open coverage/lcov-report/index.html",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "snyk-protect": "snyk protect",
    "prepublishOnly": "npm run snyk-protect",
    "configTester": "node misc/configTester.js"
  },
  "repository": {
    "url": "git+https://github.com/tagyoureit/nodejs-poolController.git"
  },
  "keywords": [
    "Pentair",
    "Hayward",
    "pool",
    "home automation",
    "solar",
    "heat",
    "temperature",
    "pump",
    "chlorinator",
    "IoT"
  ],
  "author": "Russell Goldin",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/tagyoureit/nodejs-poolController/issues"
  },
  "homepage": "https://github.com/tagyoureit/nodejs-poolController#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@zeit/next-css": "^1.0.1",
    "bluebird": "^3.5.3",
    "bootstrap": "^3.4.1",
    "bottlejs": "^1.7.2",
    "dateformat": "^3.0.3",
    "deep-diff": "^1.0.1",
    "dequeue": "^1.0.5",
    "events": "^3.0.0",
    "express": "^4.16.4",
    "getmac": "^1.4.3",
    "http-auth": "^3.1.3",
    "http-shutdown": "^1.2.0",
    "influx": "^5.0.7",
    "ip": "^1.1.5",
    "jquery": "^3.2.1",
    "jquery-ui-dist": "^1.12.1",
    "logform": "^1.9.0",
    "minimist": "^1.2.0",
    "multicast-dns": "^7.0.0",
    "mysql": "^2.15.0",
    "next": "^8.0.3",
    "node-ip": "^0.1.2",
    "node-ssdp": "^3.3.0",
    "parcel-bundler": "^1.11.0",
    "popper.js": "^1.14.7",
    "pretty-ms": "^4.0.0",
    "promised-io": "^0.3.5",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-infinite-calendar": "^2.3.1",
    "react-popper": "^1.3.3",
    "react-rangeslider": "^2.2.0",
    "react-timekeeper": "^1.1.0",
    "reactstrap": "^7.1.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.4",
    "serialport": "^6.2.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "underscore": "^1.9.1",
    "winston": "^2.4.3",
    "winston-transport": "^4.3.0",
    "yargs-parser": "^10.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "istanbul": "^0.4.5",
    "jshint": "^2.10.1",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "sinon": "^6.1.4",
    "snyk": "^1.134.2"
  },
  "snyk": true
}
