    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/dist/app.js",
            "console": "integratedTerminal",
            "outFiles": [
                "${workspaceRoot}/dist/**/*"
